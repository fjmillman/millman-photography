{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prepare": "husky install",
    "build": "prisma generate && prisma db push && npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "npm run prisma:generate && concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "tsc": "tsc -b",
    "eslint": "eslint --fix 'app/**/*.{js,jsx,ts,tsx}'",
    "prettier": "prettier --write 'app/**/*.{js,jsx,ts,tsx}'"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "app/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "@radix-ui/react-dialog": "^1.0.0",
    "@remix-run/node": "^1.6.8",
    "@remix-run/react": "^1.6.8",
    "@remix-run/vercel": "^1.6.8",
    "@vercel/node": "^2.4.4",
    "aws-sdk": "^2.1194.0",
    "bcrypt": "^5.0.1",
    "classnames": "^2.3.1",
    "cuid": "^2.1.8",
    "esbuild": "^0.14.54",
    "focus-trap-react": "^9.0.2",
    "mdx-bundler": "^9.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-mdx-images": "^2.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.8",
    "@remix-run/eslint-config": "^1.6.8",
    "@remix-run/serve": "^1.6.8",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.7.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prisma": "^4.2.1",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
