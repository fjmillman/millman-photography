{
  "name": "millman-photography",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "faunadb:run": "docker run --rm --name faunadb -p 8443:8443 fauna/faunadb",
    "faunadb:database": "fauna create-database --endpoint=localhost millman-photography",
    "faunadb:tables": "fauna eval --endpoint=localhost --file=./faunadb/tables.fql",
    "faunadb:fixtures": "fauna eval --endpoint=localhost --file=./faunadb/fixtures.fql",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.6",
    "classnames": "^2.2.6",
    "cookie": "^0.4.1",
    "faunadb": "^2.14.1",
    "focus-trap-react": "^6.0.0",
    "next": "9.4.0",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "3.0.0-canary.0",
    "next-redux-wrapper": "^6.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-optimized-image": "^0.2.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "sharp": "^0.25.3",
    "styled-jsx": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@storybook/addon-a11y": "6.0.0-rc.0",
    "@storybook/addon-actions": "6.0.0-rc.0",
    "@storybook/addon-docs": "6.0.0-rc.0",
    "@storybook/addon-links": "6.0.0-rc.0",
    "@storybook/addon-viewport": "6.0.0-rc.0",
    "@storybook/addons": "6.0.0-rc.0",
    "@storybook/react": "6.0.0-rc.0",
    "@storybook/theming": "6.0.0-rc.0",
    "@types/classnames": "^2.2.10",
    "@types/cookie": "^0.4.0",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.8",
    "@types/styled-jsx": "^2.2.8",
    "babel-loader": "^8.1.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-typescript-prettier": "^2.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "react-is": "^16.13.1",
    "storybook-mobile": "0.1.24",
    "typescript": "^3.9.2"
  }
